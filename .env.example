# Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Superhero API Configuration
# Get your API key from: https://superheroapi.com/
REACT_APP_SUPERHERO_API_KEY=your_superhero_api_key_here

# Backend API URL
# Use http://localhost:3001 for local development
REACT_APP_API_URL=http://localhost:3001

# Backend Database Configuration (for Prisma)
# SQLite database file location
DATABASE_URL="file:./api/dev.db"
